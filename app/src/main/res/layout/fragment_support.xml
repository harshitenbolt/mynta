<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorWhite"
    android:orientation="vertical"
    android:clickable="true"
    tools:context=".activity.DashboardActivity">

    <!-- TODO: Update blank fragment layout -->


<RelativeLayout
    android:layout_marginTop="@dimen/_10sdp"
    android:layout_width="match_parent"
    android:layout_marginRight="@dimen/_5sdp"
    android:layout_marginLeft="@dimen/_5sdp"
    android:orientation="horizontal"
    android:clipToPadding="true"
    android:background="@drawable/rounded_curve_corner_button_white"
    android:layout_height="@dimen/_45sdp">
    <TextView
        android:id="@+id/btPayment"
        android:layout_width="@dimen/_160sdp"
        android:gravity="center"
        android:layout_gravity="end"
        android:text="PAYMENT"
        android:textStyle="bold"
        android:layout_toStartOf="@+id/btOnboarding"
        android:layout_alignParentRight="true"
        android:layout_height="@dimen/_45sdp"
        android:background="@drawable/rounded_curve_corner_button_white"
        />

    <TextView
        android:id="@+id/btOnboarding"
        android:layout_width="@dimen/_160sdp"
        android:layout_height="@dimen/_45sdp"
        android:text="On Boarding"
        android:textAllCaps="true"
        android:textStyle="bold"
        android:gravity="center"
        android:background="@drawable/rounded_curve_corner_button"
        />


</RelativeLayout>


    <FrameLayout
        android:layout_marginTop="@dimen/_10sdp"
        android:id="@+id/flSupport"
        android:layout_width="match_parent"
        android:layout_height="match_parent">


    </FrameLayout>

</LinearLayout>